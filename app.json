{
  "expo": {
    "name": "ups-delivery-tracking",
    "description": "a precise delivery tracking app",

    "slug": "ups-delivery-tracking",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#59c1cc"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.techcitysolution.upsdelivery"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
